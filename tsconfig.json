{
	"compilerOptions": {
		"target": "es2019",
		"module": "CommonJS",
		"rootDir": "./src",
		"strict": true,
		"outDir": "./dist",
		"moduleResolution": "node",
		"esModuleInterop": true,

		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*.ts"]
}
